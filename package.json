{
  "name": "react-network-status-notifier",
  "version": "1.0.1",
  "description": "Continuously checks whether your application is online or not. Notifies when the status changes.",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Codebrahma/react-network-status-notifier"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "dependencies": {
    "@types/jest": "^24.0.17",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.9.0",
    "semantic-release-cli": "^5.1.2",
    "webpack": "^4.39.1"
  },
  "scripts": {
    "test": "jest --runInBand --coverage",
    "start": "webpack --watch",
    "build": "webpack",
    "lint": "node_modules/.bin/eslint . --fix",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "author": {
    "name": "Codebrahma",
    "email": "admin@codebrahma.com"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@testing-library/react": "^9.1.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.5.0",
    "commitizen": "^4.0.3",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.21",
    "style-loader": "^1.0.0",
    "webpack-cli": "^3.3.6"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "coverageDirectory": "./coverage/",
      "collectCoverage": true
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]nn${nextRelease.notes}"
      }
    ]
  }
}
